@inproceedings{Axelsson2012,
abstract = {A sensor fusion method for state estimation of a flexible industrial robot is developed. By measuring the acceleration at the end-effector, the accuracy of the arm angular position, as well as the estimated position of the end-effector are improved. The problem is formulated in a Bayesian estimation framework and two solutions are proposed; the extended Kalman filter and the particle filter. In a simulation study on a realistic flexible industrial robot, the angular position performance is shown to be close to the fundamental Cram\'{e}r-Rao lower bound. The technique is also verified in experiments on an ABB robot, where the dynamic performance of the position for the end-effector is significantly improved. Â© 2012 Elsevier Ltd.},
author = {Axelsson, Patrik and Karlsson, Rickard and Norrl\"{o}f, Mikael},
booktitle = {Control Engineering Practice},
doi = {10.1016/j.conengprac.2012.06.004},
file = {:C$\backslash$:/Users/matth\_000/Desktop/Research/ArmSlamPapers/FULLTEXT01.pdf:pdf},
issn = {09670661},
keywords = {Cram\'{e}r-Rao lower bound,Estimation,Extended Kalman filter,Industrial robot,Particle filter,Positioning},
number = {20},
pages = {1220--1228},
title = {{Bayesian state estimation of a flexible industrial robot}},
volume = {20},
year = {2012}
}
